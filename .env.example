# Database Configuration
POSTGRES_DB=docling_processor
POSTGRES_USER=docling_user
POSTGRES_PASSWORD=docling_pass

# Redis Configuration
REDIS_URL=redis://redis:6379

# Docling API Configuration
DOCLING_API_URL=http://localhost:8080

# Security
SECRET_KEY=your-secret-key-here-change-in-production
NEXTAUTH_SECRET=your-nextauth-secret-here

# Environment
ENVIRONMENT=development

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXTAUTH_URL=http://localhost:3000

# OAuth2 Configuration (for production)
OAUTH2_PROXY_CLIENT_ID=your-oauth2-client-id
OAUTH2_PROXY_CLIENT_SECRET=your-oauth2-client-secret
OAUTH2_PROXY_COOKIE_SECRET=your-cookie-secret

# Monitoring
PROMETHEUS_ENABLED=true
LOG_LEVEL=info